{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/tutorial/src",
  "prefix": "sandbox-nx",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/tutorial"],
      "options": {
        "project": "libs/tutorial/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/tutorial/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/tutorial/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/tutorial"],
      "options": {
        "jestConfig": "libs/tutorial/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/tutorial/**/*.ts", "libs/tutorial/**/*.html"]
      }
    }
  },
  "tags": []
}
